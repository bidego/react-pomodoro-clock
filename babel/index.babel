const SETUP = {
  Endtime: 25,
  Order: 'desc', // desc or asc (descendent)
  RemainingBeepTime: 15 //Time remaining for Beep sound
}

class Clock extends React.Component {
  constructor(props) {
    super(props);
    this.state = {
      show: '0',
      isPaused: true,
      endtime: SETUP.Endtime,
      order: SETUP.Order,
      count: 0,
      timer: 0,
    };
    this.timerId;
    this.endSound = new Audio("data:audio/mp3;base64,");
    this.beepSound = new Audio("data:audio/wav;base64,");
  }
  
  /* START Session Controls */
  playPause(e) {
    if(this.state.isPaused) {
      this.state.isPaused = false;
      this.startTimer(e);
    }
    else {
      this.state.isPaused = true;
      this.stopTimer(e);
    }
  }

  reset(size){
    this.setState({
      timer: 0,
      show: 0
    }, this.stopTimer())
    ReactDOM.render(<Clock />, document.getElementById("root"));
  }
  
  toggleOrder() {
    (this.state.order === 'desc')?this.state.order='asc':this.state.order='desc';
  }
    // START Session Endtime Control
  lessTime() {
    if(this.state.endtime>1) {
      this.state.endtime--
    }
    ReactDOM.render(<Clock />, document.getElementById("root"));
  }
    // Session Endtime Control END
  moreTime() {
    this.state.endtime++
    ReactDOM.render(<Clock />, document.getElementById("root"));
  }  
  
  startTimer(e){
    if(!this.state.timerId){
      let timerId = setInterval(()=>{
        let timer = this.state.timer + 1
        this.setState({
          timer: timer,
          timerId: timerId
        })
        
        //console.log('st: '+this.state.timer+'\nse*60: '+this.state.endtime*60);
        if(this.state.timer==1) this.playStartSound();
        
        else if(this.state.endtime*60-this.state.timer<=SETUP.RemainingBeepTime&&this.state.endtime*60-this.state.timer>=1) this.playRemainSound();
        else if(this.state.endtime*60-this.state.timer==0) {
          this.playEndSound();
          this.stopTimer();
          this.ended();
        }

        this.formatTime(e);
        ReactDOM.render(<Clock />, document.getElementById("root"));
      }, 1000)
    }
  }
  stopTimer(){
    clearInterval(this.state.timerId)     
    this.setState({timerId:null})
    this.state.isPaused = true; 
    document.getElementsByTagName("clock")[0].classList.remove("ended"); // RED Style Clock
  }
  ended(){
    document.getElementsByTagName("clock")[0].classList.add("ended"); // RED Style Clock
  }
  
  /* Session Controls END */

  formatTime(e){
    (this.state.order == 'desc')?this.state.count = this.state.endtime*60-this.state.timer:this.state.count = this.state.timer;
    if(this.state.count>=60) {
      this.state.show = Math.floor(this.state.count/60)+':';
      this.state.show += (this.state.count%60<10) ? '0'+this.state.count%60 : this.state.count%60;
    }
    else {
      this.state.show = (this.state.count<10) ? '0'+this.state.count : this.state.count;
    }
  }
  
  playStartSound() {
    this.endSound.play();
  }
  playRemainSound() {
    this.beepSound.play();
  }
  playEndSound() {
    this.endSound.play();
  }

  render() {
    return (
      <div id="container">
        <h1>REACT Pomodoro Clock</h1>
        <clock onClick={(e) => this.playPause(e)}>
          <time>{this.state.show}</time>
          <controles>
            <button onClick={(e) => { e.stopPropagation(e); this.toggleOrder(e)}}>+/-</button>
            <button onClick={(e) => { e.stopPropagation(e); this.lessTime(e)}}>-</button>
            <endtime ref="endtime">{this.state.endtime}</endtime>
            <button onClick={(e) => { e.stopPropagation(e); this.moreTime(e)}}>+</button>
            <button type="playPause" onClick={(e) => { e.stopPropagation(e); this.playPause(e)}}>&#9658; &#10073;&#10073;</button>
            <button onClick={(e) => { e.stopPropagation(e); this.reset(e) }}></button>
          </controles>
        </clock>
      </div>
    );
  }
};

ReactDOM.render(<Clock />, document.getElementById("root"));
